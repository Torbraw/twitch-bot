generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["extendedWhereUnique"]
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model AccessToken {
  userId              String             @id
  accessToken         String
  expiresIn           Int?
  obtainmentTimestamp BigInt
  refreshToken        String?
  scopes              AccessTokenScope[]
}

model AccessTokenScope {
  name         String        @id
  accessTokens AccessToken[]
}
